{
  "name": "immox",
  "description": "State management with Immer and Computed Properties",
  "version": "0.3.0",
  "main": "dist/immox.js",
  "umd:main": "dist/immox.umd.js",
  "module": "dist/immox.module.js",
  "jsnext:main": "dist/immox.module.js",
  "react-native": "dist/immox.module.js",
  "typings": "dist/index.d.ts",
  "source": "index.ts",
  "scripts": {
    "build": "microbundle",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --updateSnapshot --coverage false",
    "lint": "eslint --ignore-pattern dist/** --ext \".js,.ts,.tsx\" ."
  },
  "author": "Dalci Bagolin - dalcib",
  "license": "MIT",
  "repository": "https://github.com/dalcib/immox.git",
  "bugs": "https://github.com/dalcib/immox/issues",
  "homepage": "https://github.com/dalcib/immox#readme",
  "peerDependencies": {
    "immer": "^2.0.0 || ^3.0.0 || ^4.0.0",
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@testing-library/react-hooks": "^2.0.1",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.8.13",
    "eslint": "^6.5.1",
    "eslint-config-satya164": "^3.1.1",
    "immer": "^9.0.6",
    "jest": "^24.9.0",
    "microbundle": "^0.11.0",
    "prettier": "^1.18.2",
    "react": "^16.10.1",
    "react-dom": "^16.10.2",
    "react-hooks-testing-library": "^0.6.0",
    "react-native-testing-library": "^1.11.1",
    "react-test-renderer": "^16.10.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.4.3"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ]
  },
  "keywords": [
    "immer",
    "mobx",
    "immutability",
    "computed",
    "state"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": "satya164",
    "rules": {
      "prettier/prettier": "off",
      "jest/no-disabled-tests": "off",
      "no-console": "off",
      "no-undef": "off",
      "@typescript-eslint/no-unused-vars": "off"
    }
  },
  "dependencies": {}
}
